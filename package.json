{
  "name": "nabucasa_support_content",
  "version": "1.0.0",
  "repository": "git@github.com:nabucasa/support.git",
  "type": "module",
  "scripts": {
    "start": "npx @11ty/eleventy --serve --quiet",
    "debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "zendesk-content-preview": "npx @11ty/eleventy --serve",
    "zendesk-content-build": "rm -rf _site && npx @11ty/eleventy",
    "zendesk-content-build-preview": "rm -rf _site && PREVIEW_BUILD=1 npx @11ty/eleventy",
    "zendesk-content-deploy": "export $(grep -v -E '^\\s*#|^\\s*$' .env | sed 's/\\s*#.*//' | xargs) && node deploy.js",
    "zendesk-content-build-deploy": "npm run zendesk-content-build && npm run zendesk-content-deploy"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.0",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "cross-env": "7.0.3",
    "luxon": "3.5.0"
  }
}